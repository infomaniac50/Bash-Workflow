#!/bin/bash

function maker()
{
    PATHFILE="$BASHWF/paths/$1.txt"

    if [[ -f $PATHFILE ]]; then
        while read DIR; do
            PATH="$PATH":"$DIR"
        done < "$PATHFILE"

        echo "Loaded $PATHFILE"
        return 0
    else
        echo "Path file not found."
        return 1
    fi
}